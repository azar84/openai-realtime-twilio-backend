{
  "name": "openai-realtime-twilio-backend",
  "version": "1.0.0",
  "description": "Backend services for OpenAI Realtime Twilio integration - WebSocket server and webhook server",
  "main": "websocket-server/dist/server.js",
  "scripts": {
    "build": "cd websocket-server && npm run build",
    "start": "cd websocket-server && npm start",
    "dev": "cd websocket-server && npm run dev",
