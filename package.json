    "install-all": "npm install && cd websocket-server && npm install",
    "build-all": "npm run build",
    "deploy-webhook": "./webhook-deploy.sh",
    "setup-env": "./setup-env.sh"
  },
  "keywords": [
    "openai",
    "realtime",
    "twilio",
    "websocket",
    "voice",
    "ai"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "pg": "^8.16.3"
  },
  "devDependencies": {},
  "engines": {
    "node": ">=18.0.0"
  }
}